# #     # #表示文字标注  带可编辑文字的类型，
# #     # #！！！！注意可编辑文字的写法平面和立面！字段不同
# #     # elements:
# #     #   - conditions:
# #     #       - isHaveStone == true
# #     #     isInConstructionDrawing: true
# #     #     # 生成的文字标注和引线所在的图层
# #     #     layer: 'JK_DIM厨卫阳台地面尺寸'
# #     #     #生成标注的方向，可单个可多个
# #     #     preferDirections: [left,right]
# #     #     #sample表示在放样图中生成此标注，注意跟正常图中写法不一样
# #     #     floorPlanName: 'sample'
# #     #     #地面是只标注生成在地面图中，具体文字根据需求修改
# #     #     samplePlanName: 地面
# #     #     isStraight: true
# #     #     # 表示索引点，定义一个点的位置，引线从这个位置出发
# #     #     target:
# #     #       type: point
# #     #       data: [0,0,0]
# #     #     #此字段表示的是引用的可编辑文字的块  
# #     #     marking:
# #     #       type: symbol
# #     #       data:
# #     #         symbolName: '材料索引带标高'
# #     #         values:
# #     #           # 可编辑文字中，名称对应的具体数值
# #     #           - name: '类别'
# #     #             data: 'DZ'
# #     #           - name: '编号'
# #     #             data: '03'
# #     #           - name: '名称'
# #     #             data: '通体砖'  
# #     #           - name: '标高'
# #     #             data: '±0.000'  


# #     #   # 这里也是文字标注，是没有引用可编辑文字的纯文字标注
# #     #   - conditions:
# #     #       - sideEntry == 生成
# #     #     isInConstructionDrawing: true
# #     #     #这里的floorPlanName跟放样图是不一样，当标注不需要生成在放样图的时候需要填写这个标注具体在哪一张图中生成
# #     #     floorPlanName: 'PL-07'
# #     #     #标注的图层
# #     #     layer: 'JK_DIM地面尺寸'
# #     #     #标注生成的方向，up，down
# #     #     preferDirections: [left,right]
# #     #     isStraight: true
# #     #     #标注索引点
# #     #     target:
# #     #       type: point
# #     #       data: [0, wallWidth/2-0.002, 0]
# #     #     #这里表示纯文字的标注，text后面直接写对应文字即可
# #     #     marking:
# #     #       type: text
# #     #       data:
# #     #         - '木地板扣条'
# #     #         #如果文字需要展示两行，就把在写一个
# #     #         # - '木地板扣条'


# #     # #可以用脚本划线，定义以下字段，平面立面都可以划线，谁用写在谁的下面
# #     # # 防水线轮廓  门洞宽到原墙的部分
# #     # extraPolylines: 
# #     #   # 线的图层
# #     #   - layer: '0-防水'
# #     #     #线型
# #     #     lineType: 'ByLayer'
# #     #     #可以描述一根线的两个点
# #     #     # points: [[-totalLength/2 , -wallWidth/2 + thisfsThick, 0], [totalLength/2 , -wallWidth/2 + thisfsThick, 0]]
# #     #     #也可以描述一个轮廓
# #     #     points: [
# #     #       [-mendongLength/2 , -wallWidth/2 + nextfsThick, 0],
# #     #       [mendongLength/2 , -wallWidth/2 + nextfsThick, 0],
# #     #       [mendongLength/2, wallWidth/2 - thisfsThick, 0],
# #     #       [-mendongLength/2, wallWidth/2 - thisfsThick, 0]
# #     #     ]
# #     #     #线型的颜色值
# #     #     colorIndex: 6
# #     #     inPaperSpace: false
# #     #     # 线型的比例
# #     #     lineScale: 1
# #     #     #线的遮罩层级
# #     #     zIndex: 80

# #     # #可以在脚本里直接定义一个填充图案
# #     # # 防水填充
# #     # hatchs:
# #     #   - isInConstructionDrawing: true
# #     #     #填充的图层
# #     #     layer: '0-防水'
# #     #     #填充的图案名称，cad里面叫什么就填什么就好
# #     #     pattern: 'ANSI38'
# #     #     #填充的原点
# #     #     originPoint: [0, 0, 0]
# #     #     #填充需要旋转的角度
# #     #     angle: 0
# #     #     #填充比例
# #     #     scale: 400
# #     #     #填充颜色
# #     #     colorIndex: 251
# #     #     #填充的遮罩层级
# #     #     zIndex: 80
# #     #     #需要填充的轮廓，这里的轮廓必须是闭合的点，例如四个点确定一个矩形，正常描述四个点就行，这里需要把最后一个点闭合起来，所以要描述五个
# #     #     closePolygon: 
# #     #       -
# #     #         - [-mendongLength/2 , -wallWidth/2 + nextfsThick, 0]
# #     #         - [mendongLength/2 , -wallWidth/2 + nextfsThick, 0]
# #     #         - [mendongLength/2, wallWidth/2 - thisfsThick, 0]
# #     #         - [-mendongLength/2, wallWidth/2 - thisfsThick, 0]
# #     #         - [-mendongLength/2 , -wallWidth/2 + nextfsThick, 0]


# #     # # # 内部标尺
# #     # # dimensions:
# #     # #   - isInConstructionDrawing: true
# #     # #     #标注生成所在的图层
# #     # #     layer: 'JK_DIM交标家具尺寸'
# #     # #     #标注需要生成在哪一张图纸里面，必须要写
# #     # #     floorPlanName: 'PL-06'
# #     # #     # 表示标尺的点集，可为多个连续的点
# #     # #     points: [[-mendongLength/2, -wallWidth/2, 0],[mendongLength/2, -wallWidth/2, 0]]
# #     # #     # 表示标尺点集之间的数据，不填则默认为两点之间距离
# #     # #     overrideText: []
# #     # #     # 表示标尺的拉伸
# #     # #     # 一下数据为向上拉伸0.1m
# #     # #     textOffset: [0.2, -0.2]
# #     # #     # 表示文字方向
# #     # #     # 可选值为：up/down/left/right
# #     # #     direction: down

# #     # # # 外部标尺
# #     # # outsideDimensions:
# #     # #   -
# #     # #     - isInConstructionDrawing: true
# #     # #       # 表示标尺的点集，可为多个连续的点
# #     # #       layerNumber: 1
# #     # #       # 同dimensions
# #     # #       points: [
# #     # #         [-mendongLength/2, 0, 0],
# #     # #         [mendongLength/2, 0, 0]
# #     # #       ]
# #     # #       # 表示标尺点集之间的数据，不填则默认为两点之间距离
# #     # #       overrideText: []
# #     # #       # 表示文字方向
# #     # #       # 可选值为：up/down/left/right
# #     # #       direction: down


