# #清单  具体数值安需求写
# list:
#   # 门槛石面积
#   - conditions:
#       - isHaveStone == true
#     name: 'thresholdArea'
#     count: (mendongLength*wallWidth) + ((mendongLength + 0.06)*0.025)    

list:
  # 宽度
  - name: 'Length'
    count: mendongLength+mentaoLength*2-neigaiqaingLength*2
  - name: 'deep'
    count: wallWidth
  # 宽度
  - name: 'doorlength'
    count: mendongLength
  # 周长
  - name: 'doorpocketlength'
    count: mendongHeight*2+mendongLength
  # 门槛石面积
  - conditions:
      - isHaveStone == true
    name: 'thresholdarea'
    count: mendongLength*(wallWidth - 0)

  # 门槛石材质
  - conditions:
      - isHaveStone == true
    name: 'thresholdmaterial'
    material: doorStoneMaterial
dwgMaterial:
    project: jiali
    list:
        - conditions:
          - isHaveStone==true
          type: material
          count: mendongLength*wallWidth
          value: PJDA5TJY31SMTF12Y
generalProps:
  length:
    get: mendongLength
    set: 
      - mendongLength = value
  width:
    get: width
    set: 
      - width = value
  height:
    get: mendongHeight
    set: 
      - mendongHeight = value



