      point: [0, wallWidth/2, 0]
      direction: [0, -1, 0]
      behaviors:


        # 门槛石下沉，读取本房间下沉高度和隔壁房间下沉高度
        - 'thisRoomFloorHeight = wall.room.floorHeight'
        - 'nextRoomFloorHeight = wall.oppositeWall.room.floorHeight'

        # 关于完成面的吸附
        - 'thisfsThick = wall.fsThick'
        - 'nextfsThick = wall.oppositeWall.fsThick'
        - 'width = wall.width'
        - 'Zfhick = wall.fsThick'
        - 'Ffhick = wall.oppositeWall.fsThick'
        # 自适应天花高度
        - 'ceilHeightB = wall.room.ceilingHeight'
        - 'ceilHeightF = wall.oppositeWall.room.ceilingHeight'



