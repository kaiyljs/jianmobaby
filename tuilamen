            title: 是否生成门槛石
            value: isHaveStone
            options:
              - label: 是
                value: true
              - label: 否
                value: false


          - widget: radio
            title: 是否插入脚线
            value: IJJIAOXIAO
            options:
              - label: 是
                value: true
              - label: 否
                value: false


          # - widget: radio
          #   title: 是否生成石材脚座
