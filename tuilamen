variables:

  #这两个字段，代表离地，在popup的离地上写position.z，就不用每个子组件上写离地的变量
  - name: isInGroup 
    type: boolean
    value: false 

  - name: position
    type: point3d
    value: [0, 0, 0]

  #定义一个墙体宽度的变量
  - name: width
    type: number
    value: 0.2

  # 墙厚，用于自适应门的厚度
  - name: wallWidth
    type: number
    value: width

  # 本房间下沉值 
  - name: thisRoomFloorHeight
    type: number
    value: -0.02

  # 隔壁房间下沉值 
  - name: nextRoomFloorHeight
    type: number
    value: -0.02

  # 最小下沉值，确认门槛石具体厚度
  - name: minFloorHeight
    type: number
    value:
      - conditions:
          - thisRoomFloorHeight < nextRoomFloorHeight
        value: -thisRoomFloorHeight
      - value: -nextRoomFloorHeight

  # 本房间完成面厚度
  - name: thisfsThick
    type: number
    value: 0.02

  # 隔壁房间完成面厚度
  - name: nextfsThick
    type: number
    value: 0.02

  #--------------------天花高度-----------------------------------

  - name: ceilHeightF
    type: number
    value: 2.45 

  - name: ceilHeightB
    type: number
    value: 2.35



  #--------------------门洞规格-----------------------------------
  # 门洞长
  - name: mendongLength
    type: number
    value: 2.4  # 门套长50，盖墙20

  # 门洞高
  - name: mendongHeight
    type: number
    value: 2.2
  #--------------------门洞规格-----------------------------------



  #--------------------门框规格-----------------------------------

  # 金属板厚度
  - name: jsbanWidth
    type: number
    value: 0.08


  # 大板长度
  - name: dabanLength
    type: number
    value: wallWidth-0.045

  # 小板长度
  - name: xiaobanLength
    type: number
    value: 0.045

  # 大板厚度
  - name: dabanWidth
    type: number
    value: 0.052

  # 小板厚度
  - name: xiaobanWidth
    type: number
    value: 0.040

  # 门框与门板之间的缝隙
  - name: fengLength
    type: number
    value: 0.003

  # # 门框规格，门洞内测除去大板的长度
  # - name: menkuangLength
  #   type: number
  #   value: mendongLength - dabanLength*2 # 门套长50，盖墙20

  # # 门框规格，门洞内测除去大板的高度
  # - name: menkuangHeight
  #   type: number
  #   value: mendongHeight - dabanLength
  # #--------------------门框规格-----------------------------------



  #--------------------门套规格-----------------------------------
  # 门套自身长度
  - name: mentaoLength
    type: number
    value: 0.04

  # 门套自身厚度
  - name: mentaoWidth
    type: number
    value: 0.018

  # 门套自身厚度
  - name: zhijiaomentaoWidth
    type: number
    value: 0.017

  # 外门套盖墙的大小
  - name: neigaiqaingLength
    type: number
    value: 0.007

  # 内门套盖墙的大小
  - name: gaiqiangLength
    type: number
    value: 0.007

  # # 门套在门洞里面的大小
  # - name: neiceLength
  #   type: number
  #   value: mentaoLength - gaiqiangLength

  # # 门套高度  总高
  # - name: totalHeight
  #   type: number
  #   value: mendongHeight + 0.02

  # 门套长度 总长
  - name: totalLength
    type: number
    value: mendongHeight + gaiqiangLength*2


  # # 上门套长度  总长减去两边门套的长度
  # - name: zongSLength
  #   type: number
  #   value: mendongLength + gaiqiangLength*2 - mentaoLength*2

  #--------------------门套规格-----------------------------------


  #--------------------门板规格-----------------------------------

  # 门板下离地
  - name: menbanlidi
    type: number
    value: 0.007

  # 门板自身长度 
  - name: menbanLength
    type: number
    value: (mendongLength - 0.017*2 +0.016)/4  # 减去的是门套盖墙的距离，大板的距离和缝隙的距离，尽量用门洞一个控制多个

  # 门板自身高度  
  - name: menbanHeight
    type: number
    value: mendongHeight -0.066- menbanlidi # 减去的是门套盖墙的距离，大板的距离和缝隙的距离，尽量用门洞一个控制多个

  # 门板自身厚
  - name: menbanWidth
    type: number
    value: 0.03

  # 两门板间距
  - name: menbanJJ
    type: number
    value: 0.01

  # 门板门框宽
  - name: menbanmkWidth
    type: number
    value: 0.016
  # 门板材质
  - name: doorMaterial
    type: material
    value: PH020DXTJADG0WQQ0




  #--------------------门头板规格---------------------------------



