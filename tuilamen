#     # extraPolylines: 


#     #   # 门洞线
#     #   - layer: 'LM-门'
#     #     lineType: 'ByLayer'
#     #     inPaperSpace: false
#     #     lineScale: 0.05
#     #     points: [
#     #       [-totalLength/2, -totalWidth/2, 0], 
#     #       [ totalLength/2, -totalWidth/2, 0],
#     #       [ totalLength/2,  totalWidth/2, 0],
#     #       [-totalLength/2,  totalWidth/2, 0],
#     #       [-totalLength/2, -totalWidth/2, 0], 
#     #       ]
#     #     colorIndex: 2
#     #     zIndex: 101  

#     # #内部标尺
#     # dimensions:
#     #   -
#     #     - isInConstructionDrawing: true
#     #       # 尺寸高度标注去重
#     #       # repeatKey: 'diaogui'
#     #       layer: 'JK_DIM平面尺寸'
#     #       # 表示标尺的点集，可为多个连续的点
#     #       points: [
#     #         [-mendongLength/2 , 0, mendongHeight/3],
#     #         [mendongLength/2 , 0, mendongHeight/3]
#     #       ]
#     #       # 表示标尺点集之间的数据，不填则默认为两点之间距离
#     #       overrideText: []
#     #       # 表示标尺的拉伸
#     #       # 一下数据为向上拉伸0.1m
#     #       textOffset: [0, -0.1]
#     #       # 表示文字方向
#     #       # 可选值为：up/down/left/right
#     #       direction: up  

    #外部标尺
    outsideDimensions:
      -
        - isInConstructionDrawing: true
          # 表示标尺的点集，可为多个连续的点
          layerNumber: 1
          # 同dimensions
          points: [
            [-(mendongLength)/2, 0, 0],
            [(mendongLength)/2, 0, 0]
          ]
          # 表示标尺点集之间的数据，不填则默认为两点之间距离
          overrideText: []
          # 表示文字方向
          # 可选值为：up/down/left/right
          direction: down  


