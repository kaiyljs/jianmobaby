  # 2D视图画一个轮廓轮廓内可填充
  - type: polyline
    color: '#FFFFFF'
    # 轮廓内填充
    fill: '#FFFFFF'
    opacity: 0.5
    path: [
      [mendongLength / 2, wallWidth/2],
      [mendongLength / 2, -wallWidth/2],
      [-mendongLength / 2, -wallWidth/2],
      [-mendongLength / 2, wallWidth/2]
    ]

outline2D:
  - [
      [mendongLength / 2, wallWidth/2],
      [mendongLength / 2, -wallWidth/2],
      [-mendongLength / 2, -wallWidth/2],
      [-mendongLength / 2, wallWidth/2]
    ]
item3Ds:


          # 金属门套
  - type: extrude
    materials:
      - doorMaterial
    outline: [
        [-0.015/2 ,-wallWidth/2-0.02],
