popup:
  - widget: group
    title: 门洞规格
    tabName: 基础设置
    columns: 1
    items:

      - title: 宽度
        type: length
        value: mendongLength

      - title: 高度
        type: length
        value: mendongHeight



  - widget: group
    tabName: 基础设置
    title: 门部件信息
    items:

          # - title: 门套拼接方式
          #   type: drop
          #   inputable: false
          #   value: mentaoEntry
